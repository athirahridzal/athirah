{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  
  "title": {
    "text": "COVID-19 Vaccination",
    "subtitle":"First dose vaccination by vaccination date",
    "subtitleFontStyle":"italic",
    "subtitleFontSize":10,
    "anchor": "start",
    "color": "black"
  },
  
   "data": {
    "url": "https://api.coronavirus.dathttps://api.coronavirus.data.gov.uk/v2/data?areaType=utla&metric=cumPeopleVaccinatedFirstDoseByVaccinationDate&format=csva.gov.uk/v2/data?areaType=utla&metric=cumDeaths28DaysByDeathDate&fhttps://api.coronavirus.data.gov.uk/v2/data?areaType=utla&metric=newPeopleVaccinatedFirstDoseByVaccinationDate&format=csvormat=csv",
    "format": {"type": "csv"}
  },

   "transform":[
      
      {"filter": {
        "field": "areaName", 
      "oneOf": ["City of Edinburgh", "Coventry", "Bristol, City of"]}
      }


    ],
  
  
  "height": 300,
  "width": 310,
   
  "mark": {"type": "line", "point": false},
 
  "encoding": {

    "x": {
      "field": "date",
      "type": "temporal",
      "title": null,
      "axis": {"grid": false}
    },

    "y": {
      "field": "!!!!!",
      "type": "quantitative",
      "title": null,
      "axis": {"grid": true}
    },

    "color": {
      "field": "areaName",
      "type": "nominal",
      "scale": {"scheme": "magma"},
      "title": null,
      "legend": {
        "orient": "top-left", 
        "fillColor": "#FcFdFd"}
    }
}
}