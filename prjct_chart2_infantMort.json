{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Health",
    "subtitle": [
      "Source: World Bank"
    ],
    "subtitleFontStyle": "italic",
    "subtitleFontSize": 12,
    "anchor": "start",
    "fontSize": 16,
    "color": "orchid"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/athirahridzal/athirahridzal.github.io/main/dataInfantMortality.csv", "format":{"type": "csv"}
  },
  "transform": [
    {"filter":{"field": "year", "range":[1990,2019]}},
    {"filter":{"field": "category", "oneOf":["Russian"]}}
    ],

  "mark": "line",
  "height": 300,
  "width": "container",
  "encoding": {
    "x": {
      "field": "year",
      "type": "ordinal",
      "title": null,
      "axis": {"labelAngle": 300}
    },
    "y": {"field": "value", "type": "quantitative", "title": null},
    "color": {
      "field": "country",
      "title": null,
      "legend": {"labelFontSize": 12},
      "scale": {"scheme": "rainbow"}
    }
  }
}
